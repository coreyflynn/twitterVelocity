<div id="{{ div_string }}" class="cmap-card-wrapper {{ span_class }}">
    <div class="cmap-user-card row">
        <div class="col-xs-6">
            <h1 class="cmap-user-name">{{ name }}</h1>
            <p class="cmap-user-username">{{ username }}</p>
            <p class="cmap-user-email">{{ email }}</p>
        </div>
        <div class="col-xs-6">
            <div class="cmap-user-image-container">
                {{#each badges}}
                    <img src="{{ this }}" alt="" class="cmap-user-badge pull-right" />
                {{/each}}
            </div>
        </div>
    </div>
</div>
